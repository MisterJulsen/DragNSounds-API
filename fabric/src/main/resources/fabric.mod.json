{
  "schemaVersion": 1,
  "id": "dragnsounds",
  "version": "${version}",
  "name": "DragNSounds API",
  "description": "A simple API to upload custom sounds or music to the server and let them play in realtime to any player at any position without the need for a resourcepack. This project uses DragonLib, Architectury and JAVE2.",
  "authors": [
    "MrJulsen"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod",
    "issues": "" 
  },
  "license": "GNU General Public License v3.0",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.mrjulsen.dragnsounds.fabric.DragNSoundsFabric"
    ],
    "client": [
    ]
  },
  "mixins": [
    "dragnsounds-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.18.2",
    "java": ">=17",
    "architectury": ">=4.11.93",
    "dragonlib": ">=1.18.2-2.1.8",
    "fabric-api": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/AeSbNgvc7f",
        "JAVE2": "https://github.com/a-schild/jave2"
      },
      "badges": [ "library" ]
    }
  }
}
